
<div class="container-form">
  <h1>Edit your service 👨‍🍳</h1>

  <div class="form-edit">
    <%= simple_form_for(@chef) do |f| %>
      <%= f.input :cat_name %>
      <%= f.input :speciality, collection: ["French", "Italian", "Mexican", "American", "German", "Japanese", "Chinese"]%>
      <%= f.input :price %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
